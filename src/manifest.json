{
	"manifest_version": 3,
	"name": "Send from Gmail",
	"description": "__MSG_description__",
	"version": "1.0.0",
	"minimum_chrome_version": "88",
	"default_locale": "en",
	"background": {
		"service_worker": "background.js",
		"type": "module"
	},
	"icons": {
		"16": "icons/16.png",
		"48": "icons/48.png",
		"128": "icons/128.png"
	},
	"options_page": "options.html",
	"permissions": [
		"tabs",
		"storage",
		"contextMenus"
	]
}
